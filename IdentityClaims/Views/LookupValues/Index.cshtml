@model IEnumerable<IdentityClaims.Models.LookupValue>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Lookup.LookupCode)
            </th>
                <th>
                    @Html.DisplayNameFor(model => model.LookupValueCode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LookupValueDesc)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ActiveFlag)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Lookup.LookupCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LookupValueCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LookupValueDesc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActiveFlag)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LookupValueID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LookupValueID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LookupValueID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
